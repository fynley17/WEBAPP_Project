{
	"name": "Vue.js and PHP Development Container",
	"dockerComposeFile": "docker-compose-dev.yml",
	"service": "backend",
	"workspaceFolder": "/workspace/WEBAPP_Project",
	"forwardPorts": [80,8080],
	"customizations": {
		"extensions": [
		"felixfbecker.php-debug", // PHP debugging
		"bmewburn.vscode-intelephense-client", // PHP IntelliSense
		"ms-ceintl.vscode-language-pack-en-gb" // Optional: British English language pack
		],
		"settings": {
		"terminal.integrated.defaultProfile.linux": "bash"
		}
	}
	
}
  