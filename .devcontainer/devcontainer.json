{
  "name": "Vue + PHP Backend (Apache2)",
  "dockerComposeFile": [
    "./.devcontainer/docker-compose-dev.yml"
  ],
  "service": "frontend", 
  "workspaceFolder": "/workspace/WEBAPP_Project",
  "shutdownAction": "stopCompose",
  "extensions": [
    "dbaeumer.vscode-eslint",
    "esbenp.prettier-vscode",
    "mhutchie.git-graph"
  ],
  "postCreateCommand": "npm install --prefix frontend && composer install --working-dir backend"
}
